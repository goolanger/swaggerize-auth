        <style>
            body, html {
                height: 100%;
            }

            body {
                display: flex;
                align-items: center;
                justify-content: center;
                background-color: white;
            }

            .square-container {
                position: relative;
            }

            .square-border {
                position: absolute;
                height: 100%;
                width: 100%;
                top: 0;

                display: flex;
                align-items: center;
                justify-content: center;
            }

            .square {
                position: relative;

                width: 102px;
                height: 102px;

                transition-property: width, height;
                transition-timing-function: cubic-bezier(0, 0.3, 0, 1);
                transition-duration: .5s;
                transition-delay: 1s;
            }

            .square.display {
                width: 100%;
                height: 100%;
            }

            .border-top-left {
                border-top: 2px solid black;
                border-left: 2px solid black;

                width: 100px;
                height: 100px;

                position: absolute;
                top: 0;
                left: 0;
            }

            .border-right-bottom {
                border-right: 2px solid black;
                border-bottom: 2px solid black;

                width: 100px;
                height: 100px;

                position: absolute;
                right: 0;
                bottom: 0;
            }

            .square-content {
                position: relative;
            }

            .animate-show {
                animation-name: show;
                animation-duration: 2s;
            }

            @keyframes show {
                0%, 50% {
                    opacity: 0;
                }
                100% {
                    opacity: 1;
                }
            }
        </style>
